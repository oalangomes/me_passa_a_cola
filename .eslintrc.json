{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "globals": {
    "fetch": "readonly"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-useless-escape": "off",
    "no-empty": "off"
  }
}
